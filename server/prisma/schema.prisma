generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  userId            Int      @id @default(autoincrement())
  userName          String   @unique
  userPwd           String   
  accountDate       DateTime @default(now())
  userProfileImg    String?
  userProfileBanner String?
  userBio           String?
  userStatus        String?
}
